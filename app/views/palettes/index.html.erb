<div class="w-full">
  <% if notice.present? %>
    <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
  <% end %>

  <div id="palettes" class="my-8 min-w-full gap-8 flex flex-wrap">
    <% @palettes.each do |palette| %>
      <%= render palette %>
    <% end %>
  </div>
</div>
